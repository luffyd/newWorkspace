	public int maxProfit(int n, int m, int[] c) {
		int maxProfit = 0;
		this.m = m;
		this.n = n;
		int building = 1;
		for (int i = 0; i <= m; i++) {
			for (int j = 0; j <= m; j++) {
				int pa = i * c[(i + j) * building];
				building++;
				int pb = j * c[(i + j) * building];
				t = pa + pb;
				maxProfit = Math.max(0, calc(i, j, j + 1));
			}
		}
		return maxProfit;
	}

