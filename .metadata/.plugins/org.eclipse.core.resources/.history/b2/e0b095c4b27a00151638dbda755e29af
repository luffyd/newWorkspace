	public int maxProfit(int n, int m, int[] c) {
		int maxProfit = 0;
		this.m = m;
		this.n = n;
		for (int i = 0; i <= m; i++) {
			int a = i;
			for (int j = 0; j <= m; j++) {
				int b = j;
				int pa = i*c[(a + b) * i];
				int pb = j*c[(a + b) * j];
				t = pa + pb;
				maxProfit = Math.max(0, calc(a, b, j + 1));
			}
		}
		return maxProfit;
	}

